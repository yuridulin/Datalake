		"mobx-react-lite": "^4.0.7",
		"oidc-client-ts": "^3.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-oidc-context": "^3.1.0",
		"react-router-dom": "^6.26.0",
