{
	"ConnectionStrings": {
		"Default": "Server=${DATALAKE_HOST}; Port=2345; Database=${DATALAKE_DB}; User Id=${DATALAKE_DB_USER}; Password=${DATALAKE_DB_PASS}; Pooling=true; MinPoolSize=10; MaxPoolSize=100; Include Error Detail=True"
	},
	"StaticUsers": [
		{
			"Name": "SINUS-DEV",
			"Token": "SINUS-DEV",
			"AccessType": "Admin",
			"Host": null
		}
	],
	"Logging": {
		"LogLevel": {
			"Default": "Debug",
			"LinqToDB": "Trace",
			"LinqToDB.Command": "Trace"
		}
	},
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"LinqToDB": "Verbose",
				"LinqToDB.Command": "Verbose"
			}
		},
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"outputTemplate": "{Timestamp:dd.MM.yyyy HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
				}
			},
			{
				"Name": "File",
				"Args": {
					"path": "storage/logs/all-log-.txt",
					"rollingInterval": "Day",
					"retainedFileCountLimit": 1,
					"outputTemplate": "{Timestamp:dd.MM.yyyy HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
				}
			},
			{
				"Name": "File",
				"Args": {
					"path": "storage/logs/errors-log-.txt",
					"rollingInterval": "Day",
					"retainedFileCountLimit": 1,
					"outputTemplate": "{Timestamp:dd.MM.yyyy HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
					"restrictedToMinimumLevel": "Error"
				}
			}
		]
	},
	"Sentry": {
		"Dsn": "http://ba065b186cf44f318c99b1f59b062c27@sentry.vitebsk.energo.net:9000/19",
		"Debug": true,
		"TracesSampleRate": 1.0
	}
}
