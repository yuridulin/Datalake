{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        { "Host": "datalake-data", "Port": 8080 }
      ],
      "UpstreamPathTemplate": "/data/api/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "data"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        { "Host": "datalake-inventory", "Port": 8080 }
      ],
      "UpstreamPathTemplate": "/inventory/api/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "SwaggerKey": "inventory"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "inventory",
      "Config": [
        {
          "Name": "Inventory API",
          "Version": "v1",
          "Url": "http://datalake-inventory:8080/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "data",
      "Config": [
        {
          "Name": "Data API",
          "Version": "v1",
          "Url": "http://datalake-data:8080/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:7600"
  }
}
